{
    "useWhiteList": false,
    "serverConfigs": [
        {
            "app": "Q2T",
            "enabled": true,
            "serverAddress": "unix:${unixSocketPath}",
            "communicationType": "REST"
        },
        {
            "app": "P2P",
            "serverAddress": "http://localhost:8080"
        }
    ],
    "peer": [
        {
            "url": "http://bogus1.com"
        },
        {
            "url": "http://bogus2.com"
        }
    ],
    "keys": {
        "passwords": [],
        "keyData": [
            {
                "privateKey": {
                    "value": "PRIVATEKEY",
                    "argonOptions": {
                        "algorithm": "SHA",
                        "iterations": 1,
                        "memory": 256,
                        "parallelism": 1
                    }
                },
                "publicKey": {
                    "path": "/somepath"
                }
            }
        ]
    },
    "alwaysSendTo": []
}
